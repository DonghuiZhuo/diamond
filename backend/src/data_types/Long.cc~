uint64_t*
Client::Map(uint64_t key)
{
    return &store[key];
}

uint64_t
Client::Read(uint64_t key)
{
    return store[key];
}

void
Client::Write(uint64_t key, uint64_t value)
{
    store[key] = value;
}

